{
  "name": "user-auth-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "pnpm run migrations:run && pnpm run dev",
    "dev": "nodemon src/index.js",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all"
  },
  "keywords": [],
  "author": "Alexis Klisch <dev@alexisklisch.com>",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "2.0.20",
    "sequelize-cli": "6.5.2",
    "standard": "17.0.0"
  },
  "dependencies": {
    "@hapi/boom": "10.0.0",
    "bcryptjs": "2.4.3",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "joi": "17.7.0",
    "passport": "0.6.0",
    "passport-local": "1.0.0",
    "pg": "8.8.0",
    "pg-hstore": "2.3.4",
    "sequelize": "6.28.0"
  },
  "eslintConfig": {
    "extends": "standard"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "spaceBeforeFunctionParen": true
  }
}
